{
  "name": "mytemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn run device && react-native start",
    "device": "adb reverse --remove-all && adb -s emulator-5554 reverse tcp:8081 tcp:8081",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "yarn run device && react-native run-android --variant=release",
    "build:local": "SET ENVFILE=.env.production.local && react-native run-android --variant=release",
    "start:reset": "react-native start --reset-cache",
    "clean": "./android/gradlew clean",
    "install:pedro": "SET ENVFILE=.env.development && cd android && ./gradlew.bat app:installDebug -PreactNativeDevServerPort=8082 && cd ..",
    "test": "jest",
    "adb:reset": "adb kill-server",
    "export:aab": "cd android && SET ENVFILE=.env.production && ./gradlew bundleRelease && cd ..",
    "export:apk": "cd android && SET ENVFILE=.env.production && ./gradlew app:assembleRelease && cd ..",
    "install:app": "adb -s ad06160308f124bbc5 install ./android/app/build/outputs/apk/debug/app-debug.apk",
    "prettier": "prettier . --write",
    "clean:cache": "yarn cache clean --force",
    "linked": "react-native link react-native-svg"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/native-stack": "^6.2.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17",
    "@types/react-native": "^0.64.5",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "flipper-plugin-redux-debugger": "^2.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-flipper": "^0.112.0",
    "react-test-renderer": "17.0.1",
    "redux-flipper": "^2.0.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
